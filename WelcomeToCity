public class Hello{
  public String sayHello(String [] name, String city, String state){
    String results ="";
    String space = " ";
    for( int i=0;i<name.length;i++){
      results += space.concat(name[i]);
      }
    System.out.println(results + "hi");
    return "Hello," + results + "! Welcome to " + city + ", " + state + "!";
  }
}
